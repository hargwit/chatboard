{
    "compilerOptions": {
      "target": "es6",
      "checkJs": true,
      "forceConsistentCasingInFileNames": true,
      "noFallthroughCasesInSwitch": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "resolveJsonModule": true,
      "moduleResolution": "node",
      "baseUrl": "src"
    },
    "exclude": ["node_modules"]
  }
  